{
  "name": "PerlegoLoginScreen_Cypress",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean:reports": "rmdir /S /Q cypress\\reports && mkdir cypress\\reports && mkdir cypress\\reports\\mochareports",
    "pretest": "npm run clean:reports",
    "scripts": "cypress run --spec \"**/*.feature\"",
    "combine-reports": "mochawesome-merge cypress/reports/mocha/*.json > cypress/reports/mochareports/report.json",
    "generate-report": "marge cypress/reports/mochareports/report.json -f report -o cypress/reports/mochareports",
    "posttest": "npm run combine-reports && npm run generate-report",
    "test": "npm run scripts || npm run posttest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pirasanthan-jesugeevegan/cypress_PerlegoLoginForm.git"
  },
  "keywords": [],
  "author": "Pirasanthan Jesugeevegan",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/pirasanthan-jesugeevegan/cypress_PerlegoLoginForm/issues"
  },
  "homepage": "https://github.com/pirasanthan-jesugeevegan/cypress_PerlegoLoginForm#readme",
  "dependencies": {
    "cypress": "^6.4.0"
  },
  "devDependencies": {
    "cypress-cucumber-preprocessor": "^2.5.4",
    "cypress-multi-reporters": "^1.4.0",
    "mocha": "^8.0.1",
    "mochawesome": "^6.1.1",
    "mochawesome-merge": "^4.1.0",
    "mochawesome-report-generator": "^5.1.0"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": false
  }
}
